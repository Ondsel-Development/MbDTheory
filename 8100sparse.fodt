   <text:p text:style-name="Standard"/>
   <text:p text:style-name="Standard"/>
   <text:p text:style-name="Standard"/>
   <text:p text:style-name="Standard"><text:s text:c="27"/>- 39 -</text:p>
   <text:p text:style-name="Standard"/>
