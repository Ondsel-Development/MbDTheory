   <text:p text:style-name="Standard"/>
   <text:p text:style-name="Standard"/>
   <text:p text:style-name="Standard"/>
   <text:p text:style-name="Standard"/>
   <text:p text:style-name="Standard"><text:s text:c="23"/>June 23, 1988</text:p>
   <text:p text:style-name="Standard"/>
   <text:p text:style-name="Standard"/>
